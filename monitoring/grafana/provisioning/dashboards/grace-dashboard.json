{  
  "dashboard": {  
    "id": null,  
    "title": "Grace System Overview",  
    "tags": ["grace", "ai", "observability"],  
    "timezone": "UTC",  
    "panels": [  
      {  
        "id": 1,  
        "title": "Request Rate",  
        "type": "graph",  
        "targets": [  
          {  
            "expr": "rate(grace_actions_executed_total[5m])",  
            "legendFormat": "Actions/sec"  
          }  
        ]  
      },  
      {  
        "id": 2,  
        "title": "Error Rate",  
        "type": "graph",  
        "targets": [  
          {  
            "expr": "rate(grace_contracts_verified_total{status=\"failed\"}[5m]) / rate(grace_contracts_verified_total[5m])",  
            "legendFormat": "Error Rate"  
          }  
        ]  
      },  
      {  
        "id": 3,  
        "title": "Action Duration",  
        "type": "graph",  
        "targets": [  
          {  
            "expr": "histogram_quantile(0.95, rate(grace_action_duration_seconds_bucket[5m]))",  
            "legendFormat": "P95 Duration"  
          }  
        ]  
      },  
      {  
        "id": 4,  
        "title": "Active Missions",  
        "type": "stat",  
        "targets": [  
          {  
            "expr": "grace_missions_active",  
            "legendFormat": "Active Missions"  
          }  
        ]  
      },  
      {  
        "id": 5,  
        "title": "System Health",  
        "type": "table",  
        "targets": [  
          {  
            "expr": "up",  
            "legendFormat": "Service Status"  
          }  
        ]  
      }  
    ],  
    "time": {  
      "from": "now-1h",  
      "to": "now"  
    },  
    "refresh": "30s"  
  }  
} 
